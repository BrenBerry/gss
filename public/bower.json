{
  "name": "GSS",
  "repo": "BrenBerry/gss",
  "version": "0.0.1",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "backbone": "~1.0.0",
    "requirejs": "2.1.x",
    "requirejs-text": "~2.0.10",
    "chaplin": "~0.11.0",
    "console-polyfill": "~0.1.0",
    "lodash": "~2.1.0",
    "moment": "~2.2.x",
    "jquery": "~2.0.x",
    "semantic-ui": "*",
    "normalize-css": "~2.1.3",
    "jade": "~0.35.0"
  },
  "exportsOverride": {
    "jquery": {
      "scripts": "jquery.js"
    },
    "backbone": {
      "scripts": "backbone.js"
    },
    "requirejs": {
      "scripts": "require.js"
    },
    "requirejs-text": {
      "scripts": "text.js"
    },
    "lodash": {
      "scripts": "dist/lodash.js"
    },
    "chaplin": {
      "scripts": "chaplin.js"
    },
    "jade": {
      "scripts": "jade.js"
    },
    "moment": {
      "scripts": "moment.js"
    },
    "console-polyfill": {
      "scripts": "index.js"
    },
    "semantic-ui": {
      "scripts": "build/packaged/javascript/semantic.js",
      "styles": "build/packaged/css/semantic.css",
      "images": "build/packaged/images/**/*",
      "fonts": "build/packaged/fonts/**/*"
    },
    "normalize-css": {
      "styles": "normalize.css"
    }
  }
}
