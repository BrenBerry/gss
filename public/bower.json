{
  "name": "Combats",
  "repo": "ButuzGOL/Combats",
  "version": "0.0.1",
  "ignore": [
    "**/.*",
    "node_modules",
    "components"
  ],
  "dependencies": {
    "backbone": "~1.0.0",
    "requirejs": "2.1.x",
    "almond": "0.2.x",
    "handlebars": "1.0.x",
    "marked": "~0.2.8",
    
    "chaplin": "~0.11.0",
    "console-polyfill": "~0.1.0",
    "lodash": "~2.1.0",
    "moment": "~2.2.x",
    "jquery": "~2.0.x"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "lodash": "*",
        "jquery": "*"
      },
      "main": "backbone.js"
    },
    "moment": { "main": "moment.js" },
    "requirejs": { "main": "null" },
    "almond": { "main": "null" },
    "marked": { "main": "lib/marked.js" }
  },
  "exportsOverride": {
    "jquery": {
      "scripts": "jquery.js"
    },
    "almond": {
      "scripts": "almond.js"
    },
    "backbone": {
      "scripts": "backbone.js"
    },
    "requirejs": {
      "scripts": "require.js"
    },
    "lodash": {
      "scripts": "dist/lodash.js"
    },
    "chaplin": {
      "scripts": "chaplin.js"
    },
    "moment": {
      "scripts": "moment.js"
    },
    "console-polyfill": {
      "scripts": "index.js"
    },
    "handlebars": {
      "scripts": "handlebars.runtime.js"
    },
    "marked": {
      "scripts": "lib/marked.js"
    }
  }
}
